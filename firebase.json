{
  "hosting": {
    "public": "./dist/Campfire",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appAssociation": "AUTO",
    "rewrites": [{
        "source": "/links/**",
        "dynamicLinks": true
      },
      {
        "source": "/.well-know/apple-developer-merchantid-domain-association",
        "destination": "/apple-developer-merchantid-domain-association"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
